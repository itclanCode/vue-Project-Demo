@import "../../../style/all.styl";
.demandWrap
  height 100%
  background-color #F2F4F7
  .el-main
    padding 0
    .demand
        padding-top 50px !important
        padding-bottom 150px
        font-size 0
        background-color #ffffff
        margin 38px 324px 0
        .el-input
          width 100%!important
        .el-form-item
          margin-bottom 30px
        .el-form-item__label:before
          margin-right -10px !important;
        .el-form-item:nth-child(3) .el-form-item__content
          position relative
          .btn
            position absolute
            right 0
            top 0
            width: 90px;
            height: 34px;
            background-color: #4680ff;
            border-radius: 17px;
            font-size 14px
            letter-spacing 1px
            color #ffffff
            cursor: pointer;
            border-width 0
        .el-form-item__label
          font-size 16px
          letter-spacing 18px
          color #666666
          width 30% !important
          float left
          padding 0
          text-align right 
        .el-form-item__content
          float left
          width 50%
          margin 0 !important
          left 2%
          /* WebKit browsers */
          ::-webkit-input-placeholder
            font-size 16px
          /* Mozilla Firefox 4 to 18 */
          :-moz-placeholder
            font-size 16px
          /* Mozilla Firefox 19+ */
          ::-moz-placeholder 
            font-size 16px
          /* Internet Explorer 10+ */
          :-ms-input-placeholder
            font-size 16px
          .el-input__inner
            border-bottom 1px solid #e8e8e8
            border-left none
            border-right none
            border-top none
            border-radius 0px
            background-color rgba(255,255,255,0)
        .ruleForm1
          margin 0 auto
          width 90%
          position relative
          .show2
            position absolute
            right: 11px;
            top: 23px;
            width: 16px;
            height: 10px;
            border: 0 !important;
            padding: 0;
            background-image url('../../../assets/img/register/close.png')
            background-repeat no-repeat
            background-position center center 
            background-color #fff
            outline none
          .active
            background-image url('../../../assets/img/register/open.png')
        .argeen
          margin 46px auto 0
          width 90%
          text-align center
          .el-checkbox__inner
            background-color #4680ff
          .el-checkbox__label
            span
              letter-spacing 1px
              color #4b5169 !important
              font-size 16px
              i
                color #169bd4
        .register
          font-size 16px
          letter-spacing 1px
          width 90%
          margin 32px auto 0
          text-align center
        .bg1
          width 100%
          height 100%
          background-color #000
          position absolute
          top 0
          left 0
          z-index 2
          opacity 0.6
          display none
        .mask1  
          margin  auto
          position  absolute
          z-index 3
          top  0
          bottom  0
          left  0
          right  0
          display none
          width  520px
          height  340px
          background-color  #ffffff
          box-shadow  0px 3px 8px 0px 
            rgba(22, 155, 212, 0.22)
          img
            width 63px
            height 63px
            margin-top 62px
          p
            font-size 20px
            letter-spacing 1px
            color #666666
            margin-top 25px
          .affirm
            margin-top 50px
            width 196px
            height 51px
            background-color #169bd4
            font-size 22px
            letter-spacing 1px
            color #ffffff
        .succeed1  
          margin  auto
          position  absolute
          z-index 3
          top  0
          bottom  0
          left  0
          right  0
          display none
          width  520px
          height  350px
          background-color  #ffffff
          box-shadow  0px 3px 8px 0px 
            rgba(22, 155, 212, 0.22)
          img
            width 63px
            height 63px
            margin-top 40px
          p
            font-size 20px
            letter-spacing 1px
            color #666666
            margin-top 20px
          .success
            color #243b7f
          .affirm
            margin-top 50px
            width 196px
            height 51px
            background-color #169bd4
            font-size 22px
            letter-spacing 1px
            color #ffffff