#app {
  margin: 0;
  padding: 0;
}

html, body {
  margin: 0;
}
.tip{
  color: red;
  font-size: 13px;
  border:1px solid #4680ff !important;
}
.specil-container {
  overflow-x:hidden;
  height:100%;
  position:relative;
  .tab-report {
    text-align:left;
    font-size:13px;
    margin-bottom:15px;
    span{
      cursor: pointer;
      display: inline-block;
      width: 108px;
      height: 32px;
      line-height:32px;
      text-align:center;
      border-radius: 6px;
      -webkit-border-radius: 6px;
      border:1px solid rgba(70,128,255,1);
      color: #4680FF;
      &.active {
        background-color: #4680FF;
        color: #fff;
      }
    }
    span:last-child{
      margin-left:22px;
    }
  }
  .el-menu{
    li:hover{
      background:none !important;
      color: #409EFF !important;
    }
  }
  
  @media only screen and (max-width: 1200px) {
    .el-col-lg-8 {
        width: 32.3% !important;
    }
  }
  .no-year {
     @media only screen and (max-width: 1200px) {
    .el-col-lg-8 {
        width: 33% !important;
    }
  }
  }
  .el-icon-close{
    position: relative;
    top: -13px;
    right: -11px;
  }
  .card-container {
    height:100%;
    min-height:400px;
    position: relative;
    div.line:not(:first-child){
      border-top: 2px solid rgba(197,197,197,0.5);
    }
    div.no-margin{
      margin-left: 0px !important;
      margin-right: 0px !important;
    }
    // 特殊日期排列
    .card-clear{
      clear:both;
      margin-left:0 !important;
    }
    ul.card-content {
      padding: 0px;
      margin: 0px;
      > li {
        list-style: none;
        margin: 0px;
        padding: 0px;
        font-size:14px;
      }
    }
  .card-header{
    cursor:text;
    -moz-user-select: text;  
    -webkit-user-select: text;  
    -ms-user-select: text;
    user-select: text;
    a,.el-icon-rep-jiantou{
      color:#111;
      cursor:text;
    }
  }
  .card-header {
    padding: 0px;
    margin: 0px;
  }
  }
  div.line{
    padding-bottom: 10px !important;
  }
  .el-tabs__header {
    margin-bottom: 17px;
  }
  .el-tabs__nav-wrap {
    margin-left: 5px;
  }
  .btn-ofset-10 {
    margin-left: 10px;
  }
  .card-title {
    font-size: 16px;
    font-weight: bold;
    color: #000;
  }
  .year-date{
    padding: 0px 18px 6px;
    font-size: 20px;
    font-family:PingFangSC-Semibold;
    color:rgba(42,42,42,1);
    text-align:left;
  }
  .year-send, .year-look{
    position:relative;
    margin-bottom:2px !important;
    min-height: 201px;
    h1.head-year{
      color:#111;
      font-weight:bold;
    }
    .card-footer{
      padding-top:29px;
    }
  }
  .year-send:before, .year-look:before{
    content: '';
    position: absolute;
    top: 10px;
    left: 0px;
    width: 3px;
    height: 89%;
  }
  .year-send:before{
    background: #4680ff;
  } 
  .year-look:before{
    background:#999;
  }
  .box-card {
    padding: 15px 16px 2px 16px;
    text-align: left;
    margin: 6px 0px;
    border-radius: 10px;
    -webkit-border-radius: 10px;
    h1.head-year{
      padding-bottom:5px;
    }
    h1.head-year{
      font-size:16px;
      position: relative;
    }
    .el-icon-rep-jiantou{
        font-size:16px;
    }
    &.active, &.hasactive{
      .left-line{
        position: absolute;
        top: 20px;
        left: 13px;
        width: 3px;
        height: 82%;
      }
    }
    &.active{
      .left-line{
        background-color: #999;
      }
    }
    &.hasactive {
      .left-line{
        background-color: #4680ff;
      }
    }
    .dec {
      display: inline-block;
      height: 20px;
      line-height: 20px;
      padding: 1px 5px;
      text-align: center;
      margin: 10px 0px 0px;
      font-size: 12px;
      color: #93BE52;
      border: 1px solid #93BE52
      border-radius: 4px;
      -webkit-border-radius: 4px;
      float left;
      font-weight:bold;
    }

    .card-header {
      font-size: 16px;
      color: #000000;
      max-width:100%;
      overflow: hidden;
      text-overflow:ellipsis;
      white-space:nowrap;
      font-weight:bold;
    }

    .card-content {
      font-size: 13px;
      color: #000;
      clear: both;
      li {
        // border-bottom: 1px solid #e9e9e9 !important;
        max-width:100%;
        overflow: hidden;
        text-overflow:ellipsis;
        white-space:nowrap;
        cursor: text;
        -moz-user-select: text;  
        -webkit-user-select: text;  
        -ms-user-select: text;
        user-select:text;
        >a{
          color:#000;
          cursor: text;
        }
        >a:hover{
          cursor: text;
          user-select:value;
        }
        h2.CommissionNumber,p.CommissionNumber {
          min-width: 150px;
          display: inline-block;
          padding: 21px 0px 6px;
          font-size:13px;
        }
      }
    }
    
    .card-footer{
      width: 100%;
      min-height: 38px;
      position:relative;
      .hassend{
        position:absolute;
        right:0;
        width:100%;
        display:inline-block;
        overflow: hidden;
        text-align:right;
        cursor:pointer;
        padding: 3px 0px 8px 10px;
        font-size: 13px;
        color: #4680ff;
        i{
          position: relative;
          bottom: -3px;
          right: 0;
          font-size: 21px;
        }
      }
      .caiji {
      position:absolute;
      right:0;
      width:100%;
      overflow: hidden;
      cursor: pointer;
      padding: 3px 0px 8px 10px;
      text-align: right;
      color: #4680ff;
      font-size:13px;
      .el-icon-rep-zhifeiji{
          position: relative;
          bottom: -3px;
          right: 0;
          font-size: 21px;
          color: #4680ff;
      }
    }
    }
    
    .el-card__body {
      padding: 0px;
    }
  }

  // searchFormItem.styl
  .form-wrap {
    text-align: left;
    margin-left: 5px;
    padding: 20px 0 18px;
    position: relative;
    .el-form-item--small, .el-form-item{
      margin-bottom:0px !important;
    }
    .wtxycode {
      float: right;
    }
    .search-code{
      position:relative;
      >i.el-icon-rep-xiazai5{
          position: absolute;
          top: 10px;
          right: 10px;
          cursor:pointer;
      }
      .wtxycode{
        input{
          min-width: 380px;
          height: 32px;
          border-radius: 20px;
          border: 1px solid rgba(153,153,153,0.42);
        }
      }
    }
  }
}

// specilModel.styl
.specil-container {
    .el-dialog--center {
      padding: 17px 20px;
    }
    .loadmask {
      .el-dialog__body{
        &:before {
          content: '正在加载中....';
              display: table;
              position: absolute;
              top: 50%;
              width: 100%;
              padding: 58% 0%;
              left: 50%;
              z-index: 100000;
              font-size: 16px;
              text-align: center;
              -webkit-transform: translate(-50%, -50%);
              transform: translate(-50%, -50%);
              -webkit-transform: -webkit-translate(-50%, -50%);
              transform: -webkit-translate(-50%, -50%);
              background-color: rgba(275,275,275,0.75);
        }
      }
    }
    .el-dialog__header {
      padding: 0px;
      text-align: left;
      font-size: 20px;
      color: #000000;
    }

    .el-dialog--center .el-dialog__body {
      padding: 0px;
    }

    .el-dialog__body {
      padding: 0px;
    }

    .el-button--default {
      background: #6799D4;
      color: #fff;
      border: none;
    }
    .tableActive {
      .el-table td:first-child {
        background-color: rgba(221, 237, 255, 1) !important;
      }
    }
    .el-table td, .el-table th.is-leaf {
      border-bottom: 1px solid #c0c4cc5c;
      border-right: 1px solid #c0c4cc5c
    }
    .el-table--group{
      border:none !important;
    }

    .cancel-btn {
      background: #ddd;
      color: #666;
      border: none;
    }

    .bussin-table {
      min-height:40px;
      border: 1px solid #e5e5e5;
      margin-top:5px;
      clear: both;
      .el-table__row{
        td:first-child{
          background-color:rgba(229,240,255,1)
        }
        td:not(first-child){
          // background:rgba(255,252,235,1)
        }
      }
      .has-gutter{
        tr{
          td{
            background-color: #fff !important;
            color: #409EFF !important;
          }
        }
      }
      .el-table__header-wrapper{
        tbody{
          tr{
             td{
              background-color: none !important
            }
          }
         
        } 
      }
    }

    .prient-btn {
      display: inline-block;
      position: relative;
      width: 50px;
      height: 50px;
      line-height: 50px;
      text-align: center;
      border-radius: 50%;
      color: #fff;
      -webkit-border-radius: 50%;
      background-color: #2770e9;
      font-size: 13px;
      .el-icon-rep-dayin {
        position: absolute;
        top: 20%;
        left: 50%;
        transform: translateX(-50%);
      }
    }

    .computed-date {
      width: 308px;
      height: 40px;
      line-height: 40px;
      text-indent: 10px;
      border: 1px solid #D4D4D4;
      font-size: 13px;
      color: #000;
      text-align:left;
    }
  }

  // 基本信息
  .base-info {
    background: #F3F3F3;
    overflow: hidden;
    font-size: 13px;
    .el-table thead {
      display: none;
    }
    .el-collapse-item__header {
      .el-icon-arrow-right{
        opacity: 0;
      }
    }
    .table-content {
      border: 1px solid #e5e5e5;
    }
    .base-info-title {
      font-size: 20px;
      color: #111;
      text-align:left;
      position:relative;
      &.right:before, &.down:before {
        content: '';
        display: inline-block;
        position: absolute;
        left: 90px;
        top: 40%;
        width: 8px;
        height: 8px;
        border: 2px solid #111111ab;
        border-bottom: transparent;
        border-left: transparent;
      }
      &.right:before{
        transform: rotateZ(45deg) translateY(-50%);
        -webkit-transform: rotateZ(45deg);
         transition: transform 1s ease;
      }
       &.down:before{
        transform: rotateZ(134deg) translateY(-50%);
        -webkit-transform: rotateZ(134deg);
        transition: transform 1s ease;
      }
    }
    .el-table--enable-row-hover .el-table__body tr:hover>td{
      background-color:none !important;
    }
    .el-collapse-item__wrap{
      border:none !important;
    }
    .el-collapse-item__content{
      padding:0px !important;
      border:none !important;
    }
    .el-collapse{
      border:none !important;
    }
    tr{
      td:hover{
        background:none !important
      }
    }
    .el-table td, .specil-container .el-table th.is-leaf{
      border: none !important;
      padding:0px !important;
    }
    .el-table .cell{
      padding: 0px !important;
    }
    .el-form-item{
       border-bottom: 1px solid rgba(223,223,223,1);
      label{
        color:#111;
        font-weight:bold;
        background:rgba(229,240,255,1);
        text-align: left;
        padding-left:8px;
        border-right: 1px solid rgba(223,223,223,1);
        font-size:13px !important; 
        float:left;  
      }
      .el-form-item__content{
        span{
          padding-left:8px;
          font-size:13px !important;  
        }
      }
    }
  }
  .complete-date-content {
      margin-top:-10px;
      float: right;
      .el-form-item {
        margin-bottom: 0px !important;
      }
      .el-form-item__error {
        top: 90%;
      }
    }
  // 完成日期
  .complete-date {
    .el-form-item {
      margin: 0px !important;
    }

    .el-form-item__content {
      margin: 0px !important;
    }
    .complete-date-title {
      float: left;
    }
    .complete-date-content {
      float: right;
      padding: 26px 0px 0px;
    }
    .el-input--medium {
      .el-input__icon {
        color: #4a90e2;
      }
    }
  }

  .complete-date, .bussin-completion, .bussin-sum {
    p {
      padding: 0px;
      margin: 0px;
    }

    .complete-date-title, .bussin-completion-title, .bussin-sum-title {
      font-size: 20px;
      padding: 31px 0px 15px;
      color: #000;
      text-align:left;
    }
  }

  // 业务完成情况
  .bussin-completion {
    clear: both;
    .has-gutter {
      tr {
        th {
          border-right: 1px solid #ccc;
          background: #5C8FFF !important;
        }
      }
    }
    .el-table {
      .cell {
        font-size: 13px;
        line-height:40px;
        text-align:left;
      }

      .is-leaf {
        font-weight: bold;
      }

      th > .cell {
        color: #fff;
      }
    }
    .sum-cashed {
      width: 100%;
      height: 40px;
      color:#5C8FFF;
      text-align:left;
      font-size:13px;
      >div{
        display: inline-block;
      }
      .sum-cashed-title {
        width: 36%;
        line-height: 40px;
        text-indent: 10px;
        padding-left: 7px;
        font-size: 13px;
        font-family: PingFangSC-Semibold;
      }
      .w256{
        width: 29.6%;
      }
      .sum-cashed-active {
        width: 55.1%;
      }
      .sum-cashed-1 {
        width: 30.8%;
        line-height: 40px;
        text-indent: 10px;
        border-left: 1px solid #e5e5e5;
      }
      .sum-cashed-active-1 {
        width:177px;
      }
      .w200{
        width:22.4%;
      }
    }
  }

  .bussin-sum {
    .sum-content {
      width: 100%;
      height: 80px;
      border: 1px solid #e5e5e5;
    }

    .text-area {
      display: inline-block;
      width: 100%;
      text-align:left;
      vertical-align: bottom;
      font-size: 13px;
      min-height: 60px;
      display: block;
      resize: vertical;
      padding: 5px 15px;
      line-height: 1.5;
      -webkit-box-sizing: border-box;
      box-sizing: border-box;
      color: #606266;
      background-color: #fff;
      background-image: none;
      border: 1px solid #dcdfe6;
      border-radius: 4px;
      -webkit-transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
      transition: border-color 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);
    }
  }
  

  // 表单
   .demo-table-expand {
    font-size: 0;
    position: relative;
    .el-form-item {
      margin-right: 0;
      margin-bottom: 0;
      width: 50%;
      max-width: 100%;
  }
  label {
    width: 190px;
    position: relative;
  }
    .text-over{
        width: 50%;
        max-width: 100%;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        position:relative;
        div.el-form-item__content{
          position: absolute;
          left: 195px;
        } 
    }
    a{
      color: #666;
    }
  }
// error
 .error-page{
    position: absolute;
    width:100%;
    height:100%;
    text-align: center;
    .error-container,.refresh-container{
      position:absolute;
      left:50%;
      transform: tranlate(-50%,-50%);
      -webkit-transform: translate(-50%,-50%);
      .error-icon{
        p{
          color:#D0021B;
          &:last-child{
            padding:22px 0px 48px;
          }
          i{
            font-size:70px;
          }
        }
      }
    }
  }
  .re-link{
    display:inline-block;
    width:138px;
    height:45px;
    color:#fff;
    cursor:pointer;
  }
  // 加载
  .mc-loading{
   .el-loading-mask{
      background-color: transparent;
    }
}




