.addServer
  .tableContent
    background #f2f4f7
    text-align center
  .infoTitle
    text-align left
    margin-bottom 15px
    font-size 18px
    color #000
    font-weight bold
  .labelStyle
    width 20%
    text-align left
    font-size 14px
    color #000
    background #E5F0FF
    padding-left 10px
  .valueStyle
    background #fff
    width 30%
    text-align left
    padding-left 10px
    font-size 14px
    color #333
  .addServerTable
    width 100%
    margin-bottom 20px
    tr
      height 40px 
      line-height 40px
      td
        border 1px solid #dfdfdf
        .inputText
          width 95%
          padding-left 10px
          font-size 14px
          height 28px
          line-height 28px
          border 1px solid #e8e8e8
          border-radius 4px
        .textareaLabel
          position absolute
          top 0
          left 10px
        .textareaTd
          padding-right 5px 
          position relative
        .textarea
          margin-top 5px
          margin-bottom 5px
          color #333
          width 99.5%
  .red
    color #f56c6c
  .headerName 
    background #E5F0FF !important
  .select
    width 98.5%
  .el-collapse-item__header
    color #4680ff
  .el-collapse
    border 0
  .el-collapse-item__arrow
    float none
  .assign
    float: right;
    width: 88px;
    height: 24px;
    line-height: 24px;
    text-align: center;
    color: #4680ff;
    display: inline-block;
    cursor: pointer;
    font-size 14px
    border 1px solid #4680ff
    font-weight 400
    border-radius 5px
  .xd2
    width 100%
    table
      padding-bottom 10px
    tr
      height 40px
      line-height 40px
  .el-table th
    background #E5F0FF
  .el-table td
    position relative
  .datePicker
    width 98%
  .tipText
    position absolute 
    top 31px
    left 6%
    font-size 12px
    color #f78989
    z-index 99
  input[type="text"]:disabled
    background #f5f7fa
    cursor not-allowed
  .dialog-tr
    height 40px
    line-height 40px
  .dialog-value
    padding-left 20px
  .input-text
    width 140px
    padding-left 10px
    margin-right 10px
    font-size 14px
    height 28px
    line-height 28px
    border 1px solid #e8e8e8
    border-radius 4px
  .el-table__header
    padding-bottom 0 !important