 .index-container {
    text-align: left;
    padding-top: 0;
    font-family:PingFangSC-Regular;
    font-size: 14px;
    .el-card {
      height: 300px;
      position: relative;
      .el-card__body{
        padding: 0;
      }
      .change{
        display: inline-block;
        width: 10%;
        position: absolute;
        right :10px;
        top:20px;
        .el-radio+.el-radio{
          margin-left: 0;
          margin-top: 20px;
        }
        .x-name{
          margin-top:172px;
          display: inline-block;
          margin-left: 15px;
        }
      }
    }
    .adds{
      ul>li{
        width: 50%;
        float: left;
        .add{
          color :#4680FF;
          text-align :center;
          font-size :16px;
          font-family:PingFangSC-Semibold;
          .el-icon-rep-shangjiaokuan{
            font-size:52px;
            margin-top :20px;
            margin-bottom :24px;
            display: block;
          }

          .el-icon-rep-shenbao{
            font-size:58px;
            margin-top :20px;
            margin-bottom :20px;
            display: block;
        }
        .orange{
          color: #FF7474;
          font-size: 72px;
          margin-top :30px;
        }
      }
    }
  }
  .tablewrap{
    background:#fff;
    text-align: center;
    padding 0 16px
    box-shadow: 0 2px 12px 0 rgba(0,0,0,.1);
    -webkit-box-shadow: 0 2px 12px 0 rgba(0,0,0,.1);
    margin-top: 24px;
    text-align: left;
    .t-title{
      color:#2770E9;
      font-size: 18px;
      padding:16px 0;
      text-align: left;
    }
    .search-box{
      position absolute;
      right: 30px;
      margin-bottom:0
      .search-code {
        position: relative;
        .code {
          input {
            width: 380px;
            height: 32px;
            border-radius: 20px;
            border: 1px solid rgba(153, 153, 153, 1);
          }
        }
        .el-icon-rep-xiazai5 {
          position: absolute;
          top: 13px;
          right: 11px;
          cursor:pointer;
        }
      }
    }
    .tablestyle{
      width:100%;
      text-align: center;
      padding: 3px 10px;
      line-height: 34px;
      // margin-top: 20px;
      .reportColumn {
      background: #E5F0FF;
      color: #333;
      text-align: center;
      padding: 3px 0;
    }
    td, th{
      border: 1px solid #dfdfdf;
      .el-icon-rep-righticon01{
        margin-right: 10px;
        color: #7ED321;
        font-size: 14px;
      }
      .el-icon-rep-iconfontcuowu{
        margin-right: 10px;
        color: #848484;
        font-size: 12px;
      }
    }
    
    }
  }
 }